{
  "name": "palm-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "build-prod": "ng build --prod --aot --build-optimizer",
    "run-prod": "npm run build-prod && node server.js",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run palm-front:server:production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "compile:server": "tsc -p server.tsconfig.json",
    "compile_@agm_core": "babel node_modules/@agm/core -d node_modules/@agm/core --presets @babel/preset-env",
    "compile_@angular_core": "babel node_modules/@angular/core -d node_modules/@angular/core --presets @babel/preset-env",
    "compile_@ng_scroll": "babel node_modules/ng2-scroll-to-el -d node_modules/ng2-scroll-to-el --presets @babel/preset-env",
    "compile_@ngx_bootstrap": "babel node_modules/ngx-bootstrap -d node_modules/ngx-bootstrap --presets @babel/preset-env",
    "postinstall": "npm run compile_@agm_core && npm run compile_@angular_core && npm run compile_@ng_scroll && npm run compile_@ngx_bootstrap"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animation": "^4.0.0-beta.8",
    "@angular/animations": "^6.1.10",
