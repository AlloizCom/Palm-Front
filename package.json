    "@angular/platform-server": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@ng-bootstrap/ng-bootstrap": "^3.3.0",
    "@nguniversal/express-engine": "v6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "v6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "agm-direction": "^0.7.3",
    "angular-date-value-accessor": "0.0.2",
    "bootstrap": "^4.1.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
